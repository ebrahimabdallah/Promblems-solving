#include <iostream>
#include <queue>
using namespace std;

int main()
{
	int t, a, b;
	cin >> t;
	queue<int>myqueue;

	while (t--)
	{
		cin >> a >> b;
		if (a == 1)
		{
			myqueue.push(b);
		}
		else if (a == 2)
		{
			if (myqueue.empty())
			{
				cout << "no" << endl;
			}


			else
			{
				int que = myqueue.front();
				myqueue.pop();
				if (b == que)
				{
					cout << "yes" << endl;
				}
				else
				{
					cout << "no" << endl;
				}
			}

		}
	}
	return 0;
}
